    <form class="form-inline" action="index.html" method="post">
        <div class="row">
            <div class="form-group col-md-10">
              <label for="medTime">Medication Times</label>
              <input type="text" name="medTime" id="patientName" value="" placeholder="8am, 12pm, 5pm, etc.">
            </div>
            <div class="col-md-2">
              <input class="btn btn-primary btn-add-time" type="submit" value="Add">
            </div>
        </div>
      <!-- </div> -->
    </form>
    <!-- <div class="row"> -->
    <div class="row">
      <div class="col-xs-6">
        <ul class="timeList">
          <li>
            <button class="btn btn-block btn-info" type="submit" value="8am" name="button">8am</button>
          </li>
          <li>
            <button class="btn btn-block btn-info" type="submit" value="12pm" name="button">12pm</button>
          </li>
          <li>
